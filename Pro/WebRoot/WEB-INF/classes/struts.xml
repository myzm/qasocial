<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
   <constant name="struts.devMode" value="true" />
   <constant name="struts.i18n.encoding" value="GBK" />
   <package name="actions" extends="struts-default" namespace="/">
   		<action name="add" class="QA.QAAction" method="add">
   		    <result>/Q_add.jsp</result>
   		</action>
   		<action name="login" class="QA.loginAction" method="login">
   		    <result>/loginSuccess.jsp</result>
   		</action>
        <action name="list" class="QA.QAAction" method="List">
   		    <result>/question.jsp</result>
   		</action>
   		<action name="index">
   		    <result>/index.jsp</result>
   		</action>
   </package>
</struts>  